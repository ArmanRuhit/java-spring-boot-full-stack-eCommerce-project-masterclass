<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="myBean" class="com.armanruhit.embarx.example.car.bean.MyBean">
        <property name="message" value="I am a first bean" />
    </bean>

    <bean id="carSpecificationBeanConstructor" class="com.armanruhit.embarx.example.car.constructor.injection.Specification">
        <property name="make" value="Toyota" />
        <property name="model" value="Corolla" />
    </bean>

    <bean id="carBeanConstructor" class="com.armanruhit.embarx.example.car.constructor.injection.Car">
        <constructor-arg ref="carSpecificationBeanConstructor" />
    </bean>

    <bean id="carSpecificationBeanSetter" class="com.armanruhit.embarx.example.car.setter.injection.Specification">
        <property name="make" value="Toyota" />
        <property name="model" value="Corolla" />
    </bean>

    <bean id="carBeanSetter" class="com.armanruhit.embarx.example.car.setter.injection.Car">
        <property name="specification" value="carSpecificationBeanSetter" />
    </bean>

</beans>